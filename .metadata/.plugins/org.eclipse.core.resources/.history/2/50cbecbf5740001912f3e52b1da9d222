package learn;

interface Bool {
	boolean computeNumber(int n);	
}

interface Ints {
	int getResult(int n);
}

public class learn {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Bool  isEven = (n) -> n % 2 == 0;
		Ints multiple = (n) -> n * n;
		
		boolean result = isEven.computeNumber(3);
		int mult = multiple.getResult(6);
		
		System.out.println(result);
		System.out.println(mult);
	}

}
