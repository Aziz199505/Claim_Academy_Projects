package learn;

import java.util.Scanner;

interface Gen<T> {
	T compute(Integer t);
}




public class learn  {

	public static void main(String[] args) {
		
		/*
		 * Gen<String> condition = (n) -> { if(n % 2 != 0) return "Weird"; if((n % 2 ==
		 * 0) && ((2 <= n) && (n <= 5))) return "Not Weird"; if((n % 2 == 0) && ((6 <=
		 * n) && (n <= 20))) return "Weird"; if((n % 2 == 0) && (n > 20)) return
		 * "Not Weird"; else return null; };
		 * 
		 * System.out.println(condition.compute(22));
		 */
		String s1 = "Java";
		int x = 10;
		
		Gen<String> formatInt = (n) -> {
			String[] format = {"0","0","0"};
			
			
			char[] convertStr = (n.toString()).toCharArray();
			
			
			for(int i = convertStr.length-1; i >= 0; i--) {
				System.out.println(convertStr[i]);
			}
			return null;
		};
		 

		formatInt.compute(24);
	     
		
	}

}
