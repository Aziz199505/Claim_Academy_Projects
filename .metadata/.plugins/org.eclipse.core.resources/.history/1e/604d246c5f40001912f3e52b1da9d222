package learn;



interface Gen<T> {
	T compute(Integer t);
}

public class learn  {

	public static void main(String[] args) {
		
		Gen<String> condition = (n) -> {
			if(n % 2 != 0) return "Weird";
			if((n % 2 == 0) && ((2 <= n) && (n <= 5))) return "Not Weird";
			if((n % 2 == 0) && ((6 <= n) && (n <= 20))) return "Weird";
			if((n % 2 == 0) && (n > 20)) return "Not Weird";
			else return null;
		};
		
		System.out.println(condition.compute(5));
	}

}
