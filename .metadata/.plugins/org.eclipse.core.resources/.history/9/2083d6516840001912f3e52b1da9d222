package learn;

import java.util.Scanner;

interface Gen<T> {
	T compute(Integer t);
}




public class learn  {

	public static void main(String[] args) {
		
		/*
		 * Gen<String> condition = (n) -> { if(n % 2 != 0) return "Weird"; if((n % 2 ==
		 * 0) && ((2 <= n) && (n <= 5))) return "Not Weird"; if((n % 2 == 0) && ((6 <=
		 * n) && (n <= 20))) return "Weird"; if((n % 2 == 0) && (n > 20)) return
		 * "Not Weird"; else return null; };
		 * 
		 * System.out.println(condition.compute(22));
		 */
		String s1 = "Java";
		int x = 10;
		
		Gen<String> formatInt = (n) -> {
			
			
			
			String convertStr = n.toString();
			if(convertStr.length() == 1) return "00" + convertStr;
			if(convertStr.length() == 2) return "0" + convertStr;
			else return convertStr;
			
			
		};
		 

		System.out.println(formatInt.compute(4));
	     
		
	}

}
