package MethodLab;

import java.util.LinkedList;
import java.util.Collections;
public class MethodLab {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		System.out.println(max3(3,6,2));
		System.out.println(min3(3,6,2));
		System.out.println(median3(4,5,6));


	}
	
	public static int max3(int n1,int n2, int n3) {
		int max = n1;
		if(max < n2) max = n2;
		if(max < n3) max = n3;
		return max;
		
	}
	
	public static int min3(int n1,int n2, int n3) {
		int min = n1;
		if(min > n2) min = n2;
		if(min > n3) min = n3;
		return min;
		
	}
	
	public static int median3(int n1,int n2, int n3) {
		int mid = n1;
		if((n1 > mid && mid < n3)) mid = n2;
		if((n1 > mid && mid < n2)) mid = n3;
		return mid;
		
	}

}
